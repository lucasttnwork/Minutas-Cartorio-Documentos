  "path_transformations": {
    "dados_catalogados.*": {
      "strip_prefix": "dados_catalogados.",
      "description": "Remove prefixo dados_catalogados de campos extraidos"
    },
    "partes.outorgantes_vendedores[*].*": {
      "treat_as": "alienante[*].*",
      "description": "Trata outorgantes_vendedores como alienantes"
    },
    "partes.outorgados_compradores[*].*": {
      "treat_as": "adquirente[*].*",
      "description": "Trata outorgados_compradores como adquirentes"
    },
    "imovel.*": {
      "strip_prefix": "imovel.",
      "add_prefix": "im_",
      "description": "Normaliza campos de imovel"
    },
    "filiacao.*": {
      "flatten_to": "pn_filiacao_{key}",
      "description": "Achata estrutura de filiacao"
    },
    "domicilio.*": {
      "flatten_to": "pn_domicilio_{key}",
      "description": "Achata estrutura de domicilio"
    },
    "cndt.*": {
      "flatten_to": "pn_cndt_{key}",
      "description": "Achata estrutura de CNDT"
    },
    "sede.*": {
      "flatten_to": "pj_sede_{key}",
      "description": "Achata estrutura de sede PJ"
    },
    "administrador.*": {
      "flatten_to": "pj_admin_{key}",
      "description": "Achata estrutura de administrador"
    },
    "procurador.*": {
      "flatten_to": "pj_procurador_{key}",
      "description": "Achata estrutura de procurador"
    },
    "matricula.*": {
      "flatten_to": "im_matricula_{key}",
      "description": "Achata estrutura de matricula"
    },
    "valores.*": {
      "flatten_to": "ng_{key}",
      "description": "Achata estrutura de valores do negocio"
    }
  },
  "context_priorities": {
    "extraction": 100,
    "schema": 90,
    "ui": 60,
    "any": 50,
    "value": 10
